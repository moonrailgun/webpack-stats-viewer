    "posttest-package": "cd test/package && rm -rf node_modules package.json package-lock.json",
    "bundle-html": "node ./scripts/bundle.js",
    "prepublish": "npm run bundle-html",
    "release": "npm publish --registry https://registry.npmjs.com/"
